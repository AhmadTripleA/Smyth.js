{
  "name": "smyth.js",
  "version": "1.0.0",
  "description": "a nodejs package acting as a microservice for file handling",
  "main": "app.js",
  "scripts": {
    "dev": "concurrently \"npm run build:dev\" \"npm run start:dev\"",
    "start:dev": "nodemon --enable-source-maps --trace-deprecation --trace-warnings dist/app.js",
    "build:dev": "tsc --watch --preserveWatchOutput"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AhmadTripleA/Smyth.js.git"
  },
  "keywords": [
    "package",
    "javascript",
    "typescript",
    "microservice"
  ],
  "author": "Ahmad Aktaa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AhmadTripleA/Smyth.js/issues"
  },
  "homepage": "https://github.com/AhmadTripleA/Smyth.js#readme",
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.29",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "multer": "^2.0.0",
    "sharp": "^0.34.2",
    "zod": "^3.25.48"
  }
}
